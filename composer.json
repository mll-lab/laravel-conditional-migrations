{
    "name": "onlinepets/laravel-conditional-migrations",
    "description": "Run your Laravel migrations only when you want them to",
    "keywords": ["laravel", "migrations", "database", "cronjob", "cron", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        },
        {
            "name": "Thomas Moors",
            "email": "thomas.moors@medpets.nl"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "5.5.* || 5.6.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Onlinepets\\ConditionalMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onlinepets\\ConditionalMigrations\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/migrations"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Onlinepets\\ConditionalMigrations\\ServiceProvider"
            ]
        }
    }
}
