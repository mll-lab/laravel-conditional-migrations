{
    "name": "onlinepets/laravel-timed-migrations",
    "description": "Run your Laravel migrations at a specified time",
    "keywords": ["laravel", "migrations", "database", "cronjob", "cron", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        },
        {
            "name": "Thomas Moors",
            "email": "thomas.moors@medpets.nl"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Onlinepets\\TimedMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onlinepets\\TimedMigrations\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/migrations"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Onlinepets\\TimedMigrations\\ServiceProvider"
            ]
        }
    }
}
